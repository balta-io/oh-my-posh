{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#8625D2",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "<#9494f7> üëâ </><b>{{ .Path }}</b>",
          "type": "path"
        },
        {
          "foreground": "#11D15E",
          "style": "plain",
          "template": " ({{ .HEAD }})",
          "type": "git"
        },
        {
          "foreground": "#FF413A",
          "style": "diamond",
          "template": "<#FF413A> \u25C9</>",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "newline": false,
      "type": "prompt",
      "segments": [
        {
          "type": "dotnet",
          "style": "accordion",
          "foreground": "#8625D2",
          "background": "#FFFFFF",
          "template": "<i> Óùø {{ .Full }} </i>"
        }
      ]
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFFFFF",
          "style": "plain",
          "template": ">",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
